{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "SQS queues and S3 buckets",
  "Resources": {
    "sqsinput": {
      "Type": "AWS::SQS::Queue",
      "Properties": {
        "QueueName": "input",
        "VisibilityTimeout": 90
      }
    },
    "sqsoutput": {
      "Type": "AWS::SQS::Queue",
      "Properties": {
        "QueueName": "output",
        "VisibilityTimeout": 90
      }
    },
    "s3output": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "image-bucket-31415926"
      }
    }
  }
}